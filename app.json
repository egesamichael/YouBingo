{
  "expo": {
    "name": "You Bingo",
    "slug": "YouBingo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "youbingo",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.egesamichael.YouBingo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "react-native-google-mobile-ads": {
      "android_app_id": "ca-app-pub-8510154897428847~5866275707",
      "ios_app_id": "ca-app-pub-8510154897428847~5866275707"
    },

    "plugins": [
      "expo-tracking-transparency"

     
    ]
  },
  "web": {
    "bundler": "metro",
    "output": "static",
    "favicon": "./assets/images/favicon.png"
  },
  "plugins": [
    "expo-router"
  ],
  "experiments": {
    "typedRoutes": true
  }
}
